<Window x:Class="andrena.Usus.net.View.Dialogs.NamespaceCycleDisplay"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:local="clr-namespace:andrena.Usus.net.View.Dialogs"
		Title="{Binding Header}" Height="400" Width="600" WindowStyle="ToolWindow">
	<Window.Resources>
		<ResourceDictionary Source="..\ViewResources.xaml"/>
	</Window.Resources>
	<TabControl Style="{StaticResource BottomTabs}">
		<TabItem Header="List">
			<local:NamespaceCycleListView />
		</TabItem>
		<TabItem Header="Graph">
			<local:NamespaceCycleGraphView />
		</TabItem>
	</TabControl>
</Window>
